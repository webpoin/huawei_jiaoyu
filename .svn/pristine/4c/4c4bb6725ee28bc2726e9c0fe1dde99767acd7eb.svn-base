@charset "utf-8";
@import url(http://at.alicdn.com/t/font_1434724367_2769847.css);

html{*border:none;;overflow-y:scroll; overflow-x:auto;height: 100%;width: 100%;position: relative;}
body{margin:0;font:normal 12px/1.5 Helvetica,Arial,Tahoma,sans-serif;color:#333;width: 100%;background: #fff;}
h1,h2,h3,h4,h5,h6,ol,ul,p,form,dl{margin-top:0;margin-bottom:0}
img{border:none;vertical-align:top;}
cite,em{font-style: normal;}
ol,ul,dd{padding-left:0;margin-left:0; }
ul{list-style-type:none;}
a{text-decoration:none;white-space:nowrap;color: inherit;}
header,nav,section,article,footer,cite{display:block;margin:0;}


/*font-icon*/
i.icon{display: inline-block;vertical-align: top; font-family:"iconfont" !important;font-style:normal;-webkit-font-smoothing: antialiased;-webkit-text-stroke-width: 0.2px;-moz-osx-font-smoothing: grayscale;}



/*global*/
.fl{float:left;} .fr{float:right;}
.cs{zoom:1;} .cs:after{content:" ";display: block;visibility:hidden;height: 0;clear: both;}
.oh{overflow:hidden;} .oa{overflow:auto; }
.se{overflow:hidden;text-indent:-999em;*background:url(#); }
.tc{text-align:center;}.tl{text-align:left;}.tr{text-align:right;}.ti{text-indent:2em;}
.db{display:block;} .di{display:inline;} .dk{display:inline-block;*display:inline;*zoom:1;}
.hob{letter-spacing:-0.5em;font-family: }
.hob li,.hob a,.hob div{display:inline-block;vertical-align:top;letter-spacing:normal;}
.clear{ clear: both;}



/*header*/
.header_top{height: 80px; max-width: 1042px;margin: 0 auto;overflow: hidden;}
.header_logo {float: left;margin-top: 20px;}
.header_user{float: right;margin-top: 40px; line-height: 30px;font-size: 13px;}
.header_user a{display: inline-block;margin: 0 5px; vertical-align: middle;font-size: 16px;}

.header_nav{position: relative;height: 39px; line-height: 36px;
	background: #e8e8e8;
	background: -moz-linear-gradient(top,  #f5f5f5 0%, #dadada 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f5f5f5), color-stop(100%,#dadada));
	background: -webkit-linear-gradient(top,  #f5f5f5 0%,#dadada 100%);
	background: -o-linear-gradient(top,  #f5f5f5 0%,#dadada 100%);
	background: -ms-linear-gradient(top,  #f5f5f5 0%,#dadada 100%);
	background: linear-gradient(to bottom,  #f5f5f5 0%,#dadada 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f5f5', endColorstr='#dadada',GradientType=0 );
}
.header_nav:before{content: '';position: absolute;left: 0;top: 0;width: 100%; height: 1px;background: #dfdfdf;}
.header_nav ul{position: relative;max-width: 1042px;margin: 0 auto;overflow: hidden;}
.header_nav li{position: relative; float: left; color: #585858;font-size: 24px;border-top:3px solid transparent; }
.header_nav li a{display: block;padding: 0 26px;text-align: center;}
.header_nav li.active{border-color:#e2291c;box-shadow: 0 0 1px 1px #aaa;color: #e2291c;
	background: #ededed;
	background: -moz-linear-gradient(top,  #e8e8e8 0%, #f1f1f1 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e8e8e8), color-stop(100%,#f1f1f1));
	background: -webkit-linear-gradient(top,  #e8e8e8 0%,#f1f1f1 100%);
	background: -o-linear-gradient(top,  #e8e8e8 0%,#f1f1f1 100%);
	background: -ms-linear-gradient(top,  #e8e8e8 0%,#f1f1f1 100%);
	background: linear-gradient(to bottom,  #e8e8e8 0%,#f1f1f1 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e8e8e8', endColorstr='#f1f1f1',GradientType=0 );
	behavior:url(/jiaoyu/html/css/pie.htc);
}
.header_locatioin{height: 56px;line-height: 60px;font-size: 12px;color: #585858;max-width: 1042px;margin: 0 auto;overflow: hidden;}



.footer{ padding: 50px 0; border-top: 1px solid #8f8f8f;margin-top: 90px;font:20px/40px normal; color: #fff;background: #454545;text-align: center;}


/*main*/
.main{max-width: 1042px;margin: 0 auto;}



/*sidebar*/
.sidebar{float: left;position: relative; width: 226px;}


.sidebar_nav {position: relative; box-shadow:0 0 1px #eee;-moz-border-radius:10px 0 0 0;-webkit-border-radius:10px 0 0 0;border-radius: 10px 0 0 0;overflow: hidden;background: #fff; behavior:url(/jiaoyu/html/css/pie.htc);}
.sidebar_nav a{ display: block;position: relative;padding: 0 15px; border: 1px solid #f0f0f0; font-size: 18px;line-height: 56px;}
.sidebar_nav a i{font-size: 20px;margin-right: 8px;}
.sidebar_nav a.active{color: #e2291c;}
.sidebar_nav a.active:before{content: '';position: absolute;left: 0;top: 0;width: 5px;height: 100%; background: #e2291c; }
.sidebar_nav_arrow{display: none; position: absolute;width: 0;height: 0;top: 10px;left: 10px; overflow: hidden;border-style: solid; border-width:5px 5px 0;border-color: #ccc transparent transparent; }

.sidebar_dynamics{position: relative; margin-top: 30px;box-shadow:0 0 1px #eee;background: #fff; behavior:url(/jiaoyu/html/css/pie.htc);}
.sidebar_dynamics h2{padding: 0 15px; line-height: 60px;font-size: 18px;background: #f2f2f2; font-weight: normal;border:1px solid #e7e7e7;}
.sidebar_dynamics h2 i{font-size: 20px;margin-right: 8px;color: #0490d2;}
.sidebar_dynamics li{position: relative;padding: 15px; border:1px solid #f0f0f0;margin-top: -1px;font-size: 12px;color: #b6b6b6;}
.sidebar_dynamics h3{height: 32px;line-height: 32px;font-size: 14px;font-weight: normal;color: #333;}
.sidebar_dynamics h3 img{position: relative; width: 32px;height: 32px;-moz-border-radius:16px;-webkit-border-radius:16px;border-radius: 16px;overflow: hidden; margin-right: 10px;behavior:url(/jiaoyu/html/css/pie.htc);}
.sidebar_dynamics h4{font-size: 13px;font-weight: normal;line-height: 24px;margin-top: 6px;color: #333;}



/*
	content
	content common styls
*/
.content {margin-left: 240px;padding: 10px; border: 1px solid #f3f3f3;}
.content h2{position: relative;padding: 0 5px;margin: -10px -10px 0; font-size: 18px;line-height: 56px;  border-bottom: 3px solid #e2291c;}
.content h2 i{vertical-align: 4px;margin-right: 10px; position: relative;width: 30px;height: 30px;line-height: 30px; text-align: center;color: #fff;font-size: 16px;font-weight: normal;-moz-border-radius:15px;-webkit-border-radius:15px;  border-radius: 15px;background: #e2291c; behavior:url(/jiaoyu/html/css/pie.htc);}
.content table{width:100%; border-collapse:collapse;border:none;line-height:35px;margin-top: 20px;font-size:13px;text-align: center;}
.content th,.content td{border:1px solid #e8e8e8;line-height: 16px;padding: 12px 10px;}
.content thead th,.content thead td{ padding: 17px 10px;background: #80888a;white-space: nowrap; font-weight: bold;color: #fff;}
.content table a{white-space: normal;}


.content form{line-height: 20px;padding: 15px 0;margin-bottom: -20px;}
.content form a{display: inline-block;padding: 0 3px;}
.content form a i{margin: 0 5px; font-size: 16px;}
.content form a:hover{color: #e2291c;}
.content form a.active{color: #e2291c;}
.content form label{display: inline-block;vertical-align: middle;height: 22px;line-height: 22px; }
.content form input[type=text]{vertical-align: top; width: 94px;height: 18px;outline: none; line-height: 18px;padding: 1px 2px;border:1px solid #adadad;}
.content form button[type=submit]{position: relative; vertical-align: middle; height: 22px;border:1px solid #d6d6d6;padding: 0 10px;outline: none;
	background: #e3e3e3;
	background: -moz-linear-gradient(top,  #f7f7f7 0%, #dadada 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7f7f7), color-stop(100%,#dadada));
	background: -webkit-linear-gradient(top,  #f7f7f7 0%,#dadada 100%);
	background: -o-linear-gradient(top,  #f7f7f7 0%,#dadada 100%);
	background: -ms-linear-gradient(top,  #f7f7f7 0%,#dadada 100%);
	background: linear-gradient(to bottom,  #f7f7f7 0%,#dadada 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f7f7f7', endColorstr='#dadada',GradientType=0 );
}



.content i.icon-play2{font-size: 20px;color: #05acfa;margin-right: 0;}
.content_percent{display: block;position: relative; height:25px;margin-right: 30px;background: #dedede;border:1px solid #dedede;text-align: left;white-space: nowrap; -moz-border-radius:5px;-webkit-border-radius:5px;border-radius: 5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_percent b{display: inline-block;position: relative; height: 100%; background: #42c8e8;-moz-border-radius:5px;-webkit-border-radius:5px;border-radius: 5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_percent em{position: absolute;left: 100%;line-height: 28px;margin-left: 5px;}




.content_page{position: relative; margin-top: 20px;height: 26px;line-height: 26px;background: #fafafa;text-align: right;font-size: 14px;}
.content_page a{display: inline-block;position: relative; width: 26px;text-align: center; margin: 0 10px;}
.content_page a.active{background: #7dacd9;-moz-border-radius:13px;-webkit-border-radius:13px;border-radius: 13px;color: #fff;behavior:url(/jiaoyu/html/css/pie.htc);}



/*learning map*/
.content_map table {margin-bottom: 10px;}
.content_map th {position: relative;text-align: left;font-weight: normal;}
.content_map th i.icon{position: absolute;left: 0px; top: 50%;margin-top: -10px; margin-right: 8px;font-size: 12px;}
.content_map tr.level_0 th {padding-left: 25px;}
.content_map tr.level_0 th i{left: 8px; color: #333;}
.content_map tr.level_1{background: #f2f3f3}
.content_map tr.level_1 th{padding-left: 40px;}
.content_map tr.level_1 th i{left: 23px; color: #929393;}
.content_map tr.level_2{background: #f2f3f3;color: #757575;}
.content_map tr.level_2 th{padding-left: 55px;}
.content_map tr.level_2 th i {left: 38px; margin-top: -3px; width: 6px;height: 6px;-moz-border-radius:3px;-webkit-border-radius:3px;border-radius: 3px;background: #929393;behavior:url(/jiaoyu/html/css/pie.htc);}



.content_course table a{color: #42c8e8;text-decoration: underline;}


.content_forum {}
.content_forum_man{margin: 20px 18px 0; padding: 25px;overflow: hidden; background: #f7fafd;border:1px solid #eee;}
.content_forum_man dl{float: left;width: 50%;}
.content_forum_man dt{display: inline-block;position: relative;margin: 0 8px;}
.content_forum_man dt a{display: block;}
.content_forum_man dt img{width: 110px;height: 110px;-moz-border-radius:55px;-webkit-border-radius:55px;border-radius: 55px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum_man dt cite{display: none; position: absolute;width: 100%;  left: 0;top: 0;color: #fff;font-size: 18px; background: rgba(226,41,28,0.3);line-height: 30px;padding: 25px 0;text-align: center; border-radius: -moz-border-radius:55px;-webkit-border-radius:55px;border-radius: 55px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum_man dt cite i{display: block; font-size: 32px;}
.content_forum_man dt a:hover cite{display: block;}
.content_forum_man dd {display: inline-block;vertical-align: bottom;margin: 0 8px;font-size: 14px;line-height: 22px;}


.content_forum form{overflow: hidden; margin: 16px 18px 0; padding: 8px 8px 0; background: #f7fafd;border:1px solid #eee;}
.content_forum form cite{display: inline-block;}
.content_forum form select{width: 138px;height: 18px;margin-right: 8px;margin-bottom: 10px; background: #fff; vertical-align: middle;border-radius: 0;border:1px solid #dcdcdc; line-height: 18px;padding: 8px 0;}
.content_forum form cite{display: inline-block;margin-bottom: 8px; position: relative;width: 218px; line-height: 26px;  height: 26px; vertical-align: middle;text-align: left;overflow: hidden;}
.content_forum form cite input[type=text]{position: absolute;right: 0; top:0;height: 18px; margin: 0;border:1px solid #ddd;color: #999; outline:none;width: 180px; background:#fff;line-height: 18px;padding: 3px 26px 3px 10px;border-radius:13px;  behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum form cite button[type=submit]{position: absolute;right:0;top: 0;margin: 0;padding: 0;border:none;background: none;font-size: 14px;line-height: 26px; width: 26px;height: 26px; outline:none; overflow: hidden;cursor:pointer;}
.content_forum form a.content_forum_posts{display: inline-block;height: 26px;line-height: 26px;padding: 0 20px;margin-bottom: 8px; color: #fff; background: #e2291c;}

.content_forum_table{margin: 0 18px;}
.content_forum_table table{color: #b2b2b2;font-size: 12px;text-align: left;}
.content_forum_table table td{border:none;border-bottom: 1px dashed #ddd;}
.content_forum_table i.icon-message{position: relative;height: 18px;line-height: 24px; font-size: 12px;}
.content_forum_table i.icon-message:before{font-size: 36px;color: #f0eeee;text-shadow:0 0 1px #1B1B1B;}
.content_forum_table i.icon-message b{position: absolute;left: 0;top: 0;width: 100%; font-weight: normal;color: #05acfa; text-align: center; text-align: center;}
.content_forum_table table h3{ position: relative; font-size: 16px;color: #333;font-weight: 400;}
.content_forum_table table em{display: block;text-align: center;color: #333;}
.content_forum_table tr.hot h3{display: inline-block;margin-right: 20px; color: #e2291c;}
.content_forum_table tr.hot h3:after{content: 'TOP';position: absolute;right: -30px; top: -5px;margin-left: 5px; width: 28px;height: 16px;line-height: 16px; font-size: 12px;color: #fff;text-align: center;background: #f97552; -moz-border-radius:2px;-webkit-border-radius:2px;border-radius: 2px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum_table tr.info h3{color: #05acfa;}













