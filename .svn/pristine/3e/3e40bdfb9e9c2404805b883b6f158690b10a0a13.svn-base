@charset "utf-8";
@import url(iconfont/iconfont.css);



html{*border:none;;overflow-y:scroll; overflow-x:auto;height: 100%;width: 100%;position: relative;}
body{margin:0;font:normal 12px/1.5 Helvetica,Arial,Tahoma,sans-serif;color:#333;width: 100%;background: #fff;}
h1,h2,h3,h4,h5,h6,ol,ul,p,form,dl{margin-top:0;margin-bottom:0}
img{border:none;vertical-align:top;}
cite,em{font-style: normal;}
ol,ul,dd{padding-left:0;margin-left:0; }
ul{list-style-type:none;}
a{text-decoration:none;white-space:nowrap;color: inherit;}
header,nav,section,article,footer,cite{display:block;margin:0;}


/*font-icon*/
i.icon{display: inline-block;vertical-align: top; font-family:"iconfont" !important;font-style:normal;-webkit-font-smoothing: antialiased;-webkit-text-stroke-width: 0.2px;-moz-osx-font-smoothing: grayscale;}


ul.list{margin: 0 0 10px 25px;padding: 0;list-style-type: disc;}
ul.list li{  display: list-item;}

ol.list{margin-left: 25px;}


/*global*/
.fl{float:left;} .fr{float:right;}
.cs{zoom:1;} .cs:after{content:" ";display: block;visibility:hidden;height: 0;clear: both;}
.oh{overflow:hidden;} .oa{overflow:auto; }
.se{overflow:hidden;text-indent:-999em;*background:url(#); }
.tc{text-align:center;}.tl{text-align:left;}.tr{text-align:right;}.ti{text-indent:2em;}
.db{display:block;} .di{display:inline;} .dk{display:inline-block;*display:inline;*zoom:1;}
.hob{letter-spacing:-0.5em;font-family: }
.hob li,.hob a,.hob div,.hob span,.hob dl{display:inline-block;vertical-align:top;letter-spacing:normal;}
.clear{ clear: both;}

.btn{display: inline-block;background: #8bc942;text-align: center;line-height: 16px;padding: 6px 10px;color: #fff;}
.btn_orange{background: #f58a22;}
.btn_blue{background: #05acfa;}
.btn_green{background: #8bc942;}

.show_min{display: none;}


/*header*/
.header_top{height: 80px; max-width: 1042px;margin: 0 auto;overflow: hidden;}
.header_logo {float: left;margin-top: 20px;}
.header_user{float: right;margin-top: 40px; line-height: 30px;font-size: 13px;}
.header_user a{display: inline-block;margin: 0 5px; vertical-align: middle;font-size: 16px;}

.header_nav{position: relative;height: 39px; line-height: 36px;
	background: #e8e8e8;
	background: -moz-linear-gradient(top,  #f5f5f5 0%, #dadada 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f5f5f5), color-stop(100%,#dadada));
	background: -webkit-linear-gradient(top,  #f5f5f5 0%,#dadada 100%);
	background: -o-linear-gradient(top,  #f5f5f5 0%,#dadada 100%);
	background: -ms-linear-gradient(top,  #f5f5f5 0%,#dadada 100%);
	background: linear-gradient(to bottom,  #f5f5f5 0%,#dadada 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f5f5f5', endColorstr='#dadada',GradientType=0 );
}
.header_nav:before{content: '';position: absolute;left: 0;top: 0;width: 100%; height: 1px;background: #dfdfdf;}
.header_nav ul{position: relative;max-width: 1042px;margin: 0 auto;overflow: hidden;}
.header_nav li{position: relative; float: left; color: #585858;font-size: 24px;border-top:3px solid transparent; }
.header_nav li a{display: block;padding: 0 26px;text-align: center;}
.header_nav li.active{border-color:#e2291c;box-shadow: 0 0 1px 1px #aaa;color: #e2291c;
	background: #ededed;
	background: -moz-linear-gradient(top,  #e8e8e8 0%, #f1f1f1 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e8e8e8), color-stop(100%,#f1f1f1));
	background: -webkit-linear-gradient(top,  #e8e8e8 0%,#f1f1f1 100%);
	background: -o-linear-gradient(top,  #e8e8e8 0%,#f1f1f1 100%);
	background: -ms-linear-gradient(top,  #e8e8e8 0%,#f1f1f1 100%);
	background: linear-gradient(to bottom,  #e8e8e8 0%,#f1f1f1 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e8e8e8', endColorstr='#f1f1f1',GradientType=0 );
	behavior:url(/jiaoyu/html/css/pie.htc);
}
.header_locatioin{height: 56px;line-height: 60px;font-size: 12px;color: #585858;max-width: 1042px;margin: 0 auto;overflow: hidden;}



.footer{ padding: 50px 0; border-top: 1px solid #8f8f8f;margin-top: 90px;font:20px/40px normal; color: #fff;background: #454545;text-align: center;}


/*main*/
.main{max-width: 1042px;margin: 0 auto;}



/*sidebar*/
.sidebar{float: left;position: relative; width: 226px;}


.sidebar_nav {position: relative; box-shadow:0 0 2px #ccc;-moz-border-radius:10px 0 0 0;-webkit-border-radius:10px 0 0 0;border-radius: 10px 0 0 0;overflow: hidden;background: #fff; behavior:url(/jiaoyu/html/css/pie.htc);}
.sidebar_nav a{ display: block;position: relative;padding: 0 15px; border: 1px solid #f0f0f0; font-size: 18px;line-height: 56px;}
.sidebar_nav a i{font-size: 20px;margin-right: 8px;}
.sidebar_nav a.active{color: #e2291c;}
.sidebar_nav a.active:before{content: '';position: absolute;left: 0;top: 0;width: 5px;height: 100%; background: #e2291c; }
.sidebar_nav_arrow{display: none; width: 0;height: 0; text-align: right;margin: 13px 10px 14px auto; overflow: hidden;border-style: solid; border-width:10px 10px 0;border-color: #999 transparent; }
.sidebar_nav.on .sidebar_nav_arrow{border-width: 0 10px 10px}

.sidebar_dynamics{position: relative; margin-top: 30px;box-shadow:0 0 1px #eee;background: #fff; behavior:url(/jiaoyu/html/css/pie.htc);}
.sidebar_dynamics h2{padding: 0 15px; line-height: 60px;font-size: 18px;background: #f2f2f2; font-weight: normal;border:1px solid #e7e7e7;}
.sidebar_dynamics h2 i{font-size: 20px;margin-right: 8px;color: #0490d2;}
.sidebar_dynamics li{position: relative;padding: 15px; border:1px solid #f0f0f0;margin-top: -1px;font-size: 12px;color: #b6b6b6;}
.sidebar_dynamics h3{height: 32px;line-height: 32px;font-size: 14px;font-weight: normal;color: #333;}
.sidebar_dynamics h3 img{position: relative; width: 32px;height: 32px;-moz-border-radius:16px;-webkit-border-radius:16px;border-radius: 16px;overflow: hidden; margin-right: 10px;behavior:url(/jiaoyu/html/css/pie.htc);}
.sidebar_dynamics h4{font-size: 13px;font-weight: normal;line-height: 24px;margin-top: 6px;color: #333;}



/*
	content
	content common styls
*/
.content {margin-left: 240px;padding: 10px; border: 1px solid #f3f3f3;}
.content>h2{position: relative;padding: 0 5px;margin: -10px -10px 0; font-size: 18px;line-height: 56px;  border-bottom: 3px solid #e2291c;}
.content>h2 i{vertical-align: 4px;margin-right: 10px; position: relative;width: 30px;height: 30px;line-height: 30px; text-align: center;color: #fff;font-size: 16px;font-weight: normal;-moz-border-radius:15px;-webkit-border-radius:15px;  border-radius: 15px;background: #e2291c; behavior:url(/jiaoyu/html/css/pie.htc);}
.content h2.color_green{color:#03c287;border-color:#03c287;}
.content h2.color_green i{background: #03c287;}
.content h2.color_purple{color:#005dba;border-color:#005dba;}
.content h2.color_purple i{background: #005dba;}
.content h2.color_orange{color:#f96918;border-color:#f96918;}
.content h2.color_orange i{background: #f96918;}
.content h2.color_blue{color:#42c8e8;border-color:#42c8e8;}
.content h2.color_blue i{background: #42c8e8;}
.content h2.color_orchid{color:#00a9a6;border-color:#00a9a6;}
.content h2.color_orchid i{background: #00a9a6;}


.content table{width:100%; border:none;line-height:35px;margin-top: 20px;font-size:13px;text-align: center;border-style: solid ;border-width:1px 1px 0 0 ;border-color:#e8e8e8; }
.content th,.content td{position: relative; font-weight: normal;line-height: 16px;padding: 12px 10px;border-style: solid ;border-width:0 0 1px 1px;border-color:#e8e8e8; }
.content thead th,.content thead td{ padding: 17px 10px; font-weight: bold;color: #fff;background-color: #80888a;}
.content tbody tr:nth-child(even) td,.content tbody tr:nth-child(even) th {background-color: #f1f3f3}
.content table a{white-space: normal;}
.content th {text-align: left;}
.content thead a{display: inline-block;position: relative;padding-right: 12px;}
.content thead a i{position: absolute;right: 0;top: 50%;margin-top: -10px; height: 20px;line-height: 20px;}
.content a.btn{white-space: nowrap;min-width: 80px;}


.content form{line-height: 20px;padding: 15px 0;margin-bottom: -20px;overflow: hidden;}
.content form a{display: inline-block;padding: 0 3px;}
.content form a i{margin: 0 5px; font-size: 16px;}
.content form a:hover{color: #e2291c;}
.content form a.active{color: #e2291c;}
.content form a.back i{font-size: 24px;margin-top: 2px;color: #e2291c;}
.content form b i{ font-weight: normal; margin: 0 5px; font-size: 16px;}
.content form strong b{font-size: 16px;color: #e2291c;}
.content form strong .content_percent{display: inline-block;vertical-align: middle; width: 200px;}
.content form label{display: inline-block;vertical-align: middle;height: 22px;line-height: 22px; }
.content form input[type=text]{vertical-align: top; width: 94px;height: 18px;outline: none; line-height: 18px;padding: 1px 2px;border:1px solid #adadad;}
.content form button[type=submit]{position: relative; vertical-align: middle; height: 22px;border:1px solid #d6d6d6;padding: 0 10px;outline: none;
	background: #e3e3e3;
	background: -moz-linear-gradient(top,  #f7f7f7 0%, #dadada 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#f7f7f7), color-stop(100%,#dadada));
	background: -webkit-linear-gradient(top,  #f7f7f7 0%,#dadada 100%);
	background: -o-linear-gradient(top,  #f7f7f7 0%,#dadada 100%);
	background: -ms-linear-gradient(top,  #f7f7f7 0%,#dadada 100%);
	background: linear-gradient(to bottom,  #f7f7f7 0%,#dadada 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f7f7f7', endColorstr='#dadada',GradientType=0 );
}



.content i.icon-play2{font-size: 20px;color: #05acfa;margin-right: 0;}
.content_percent{display: block;position: relative; min-width: 100px;  height:25px;margin-right: 30px;background: #dedede;border:1px solid #dedede;text-align: left;white-space: nowrap; -moz-border-radius:5px;-webkit-border-radius:5px;border-radius: 5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_percent b{display: inline-block;position: relative; height: 100%;text-indent: 10px;line-height: 23px;color: #fff;font-weight: normal;white-space: nowrap; background: #42c8e8;-moz-border-radius:5px;-webkit-border-radius:5px;border-radius: 5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_percent em{position: absolute;left: 100%;line-height: 28px;margin-left: 5px;}

.content_percent_inside .content_percent{margin-right: 0;}
.content_percent_inside .content_percent em{left: 0;color: #fff;}
.content_percent_orchid .content_percent b{background: #00a9a6;}



.content_page{position: relative; margin-top: 20px;height: 26px;line-height: 26px;background: #fafafa;text-align: right;font-size: 14px;}
.content_page a{display: inline-block;position: relative; width: 26px;text-align: center; margin: 0 10px;}
.content_page a.active{background: #7dacd9;-moz-border-radius:13px;-webkit-border-radius:13px;border-radius: 13px;color: #fff;behavior:url(/jiaoyu/html/css/pie.htc);}



/*learning map*/
.content_map table {margin-bottom: 10px;}
.content_map tbody th,.content_map tbody td{ border-top: none;background: #f2f3f3;-webkit-transition: all 0.1s; transition: all 0.1s;overflow: hidden;position: relative;}
.content_map th i.icon{position: absolute;left: 0px; top: 50%;margin-top: -10px;cursor: pointer; margin-right: 8px;font-size: 12px;}
.content_map tr.level_0 th {padding-left: 25px;background: #fff;}
.content_map tr.level_0 th i{left: 8px; color: #333;}
.content_map tr.level_1 th{padding-left: 40px;}
.content_map tr.level_1 th i{left: 23px; color: #929393;}
.content_map tr.level_2{color: #757575;}
.content_map tr.level_2 th{padding-left: 55px;}
.content_map tr.level_2 th i {left: 38px; margin-top: -3px; width: 6px;height: 6px;-moz-border-radius:3px;-webkit-border-radius:3px;border-radius: 3px;background: #929393;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_map tr.hide th,.content_map tr.hide td{border-bottom:none;padding-top: 0;padding-bottom: 0; height: 0;line-height: 0;}




.content_forum_man{position: relative; margin: 20px 18px 0; padding: 25px;overflow: hidden; background: #f7fafd;border:1px solid #eee;}
.content_forum_man dl{float: left;width: 50%;}
.content_forum_man dt{display: inline-block;position: relative;margin: 0 8px;}
.content_forum_man dt a{display: block;position: relative;width: 110px;height: 110px;}
.content_forum_man dt img{position: relative;width: 110px;height: 110px;-moz-border-radius:55px;-webkit-border-radius:55px;border-radius: 55px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum_man dt cite{display: none; position: absolute;width: 100%;  left: 0;top: 0;color: #fff;font-size: 18px; background: rgba(226,41,28,0.3);line-height: 30px;padding: 25px 0;text-align: center; border-radius: -moz-border-radius:55px;-webkit-border-radius:55px;border-radius: 55px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum_man dt cite i{display: block; font-size: 32px;}
.content_forum_man dt a:hover cite{display: block;}
.content_forum_man dd {display: inline-block;vertical-align: bottom;margin: 0 8px;font-size: 14px;line-height: 22px;color: #999;}
.content_forum_man dd strong{display: block;margin-bottom: 5px;margin-top: 5px; color: #333;}


.content_forum form{position: relative; overflow: hidden; margin: 16px 18px 0; padding: 8px 8px 0; background: #f7fafd;border:1px solid #eee;}
.content_forum form cite{display: inline-block;}
.content_forum form select{width: 138px;margin-right: 8px;margin-bottom: 10px; background: #fff; vertical-align: middle;border-radius: 0;border:1px solid #dcdcdc;}
.content_forum form cite{display: inline-block;margin-bottom: 8px; position: relative;width: 218px; line-height: 26px;  height: 26px; vertical-align: middle;text-align: left;overflow: hidden;}
.content_forum form cite input[type=text]{position: absolute;right: 0; top:0;height: 18px; margin: 0;border:1px solid #ddd;color: #999; outline:none;width: 180px; background:#fff;line-height: 18px;padding: 3px 26px 3px 10px;border-radius:13px;  behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum form cite button[type=submit]{position: absolute;right:0;top: 0;margin: 0;padding: 0;border:none;background: none;font-size: 14px;line-height: 26px; width: 26px;height: 26px; outline:none; overflow: hidden;cursor:pointer;filter:none;}
.content_forum form cite button i{line-height: 26px;}
.content_forum form a.content_forum_posts{display: inline-block;height: 26px;line-height: 26px;padding: 0 20px;margin-bottom: 8px; color: #fff; background: #e2291c;}

.content_forum_li{margin: 0 18px;}
.content_forum_li ul{color: #b2b2b2;font-size: 12px;text-align: left;}
.content_forum_li li{position: relative; padding: 20px 0; border:none;border-bottom: 1px dashed #ddd;overflow: hidden;}
.content_forum_li ul h6{float: left;width: 45px;font-weight: normal;text-align: center;}
.content_forum_li ul h4{float: right;width: 90px;font-weight: normal;}
.content_forum_li ul h4 i{margin-right: 3px;}
.content_forum_li ul h4 span{display: block;}
.content_forum_li ul h5{float: right;width: 70px;font-weight: normal;}
.content_forum_li ul cite{margin-left: 50px;margin-right: 180px;}
.content_forum_li a{white-space: normal;}
.content_forum_li i.icon-message{position: relative;height: 18px;line-height: 24px; font-size: 12px;}
.content_forum_li i.icon-message:before{font-size: 40px;color: #EDEDED;}
.content_forum_li i.icon-message b{position: absolute;left: 0;top: -1px;width: 100%; font-weight: normal;color: #05acfa; text-align: center; text-align: center;}
.content_forum_li h3{ position: relative; font-size: 16px;color: #333;font-weight: 400;}
.content_forum_li h3 a{display: block;max-width: 100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;padding-right: 10px;}
.content_forum_li em{display: block;text-align: center;color: #666;}
.content_forum_li li.hot h3{display: inline-block; margin-right: 20px;max-width: 100%; color: #e2291c;}
.content_forum_li li.hot h3:after{content: 'TOP';position: absolute;right: -20px; top: -5px;margin-left: 5px; width: 28px;height: 16px;line-height: 16px; font-size: 12px;color: #fff;text-align: center;background: #f97552; -moz-border-radius:2px;-webkit-border-radius:2px;border-radius: 2px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_forum_li li.info h3{color: #05acfa;}



.content_course tbody a{color: #42c8e8;}
.content_course tbody a:hover{text-decoration: underline;}




.content_course_arrow{display: block!important; margin: 10px 0; font-size: 30px;color: #858585;}
.content_course_study{text-align: center;margin: 30px -20px 10px;}
.content_course_study dl{display: inline-block;vertical-align: top;width: 25%;  position: relative; text-align: center;}
.content_course_study dt{margin: 0 30px; height: 30px;border-bottom: 1px dashed #c4c4c4; line-height: 30px;margin-bottom: 8px;white-space: nowrap; font-size: 16px;z-index: 1; background: #fff;}
.content_course_study dd{position: relative;margin: 0 30px; border: 1px solid #d6d6d6;font-size: 14px;}
.content_course_study dd.content_course_arrow{border:none;margin: 0;height: 30px;background: url(../images/course_study_arrow.png) no-repeat center center;}
.content_course_study h3{position: relative;font-weight: normal;border-bottom: 1px solid #d6d6d6;}
.content_course_study h3 img{vertical-align: top;width: 100%;}
.content_course_study h3 span{position: absolute;left: 0;top: 0;white-space: nowrap; width: 100%;height: 100%;color: #fff;
	background: rgba(0,0,0,0.3);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#66000000,endColorstr=#66000000);
}
.content_course_study h3 span:before{content: '';display: inline-block;width: 1px;height: 100%;vertical-align: middle;}
.content_course_study h3 span em{display: inline-block;vertical-align: middle;}
.content_course_study ul{position: relative; }
.content_course_study ul:before{content: '';position: absolute;left: 50%;top: 50%;margin-left: -0.5px;margin-top: -13px; width: 1px;height: 26px;background: #d6d6d6;}
.content_course_study ul:after{content: '';display: block;width: 100%;height: 0;line-height: 0;overflow: hidden;}
.content_course_study ul i{font-size: 16px;margin-right: 3px;}
.content_course_study li{float: left;width: 50%;white-space: normal;line-height: 16px;padding: 5px 0;}
.content_course_study h6{position: absolute;left: 0;top: -28px;width: 100%;height: 28px;line-height: 28px; text-align: center;color: #858585;font-weight: normal;font-size: 20px;}
.content_course_study cite{display: none;position: absolute;left: -1px;top: 100%;padding: 5px 0; margin-top: 0px;line-height: 20px;font-size: 12px; width: 100%;z-index: 1; background: #fff; border:1px solid #d6d6d6;}
.content_course_study cite a{display: block;text-align: left;margin: 0 5px;overflow: hidden;text-overflow: ellipsis;}
.content_course_study cite i{margin-right: 3px;font-size: 12px;}
.content_course_study dl:hover{z-index: 2;}
.content_course_study dl:hover dt span{color: #e2291c;border-color: #e2291c;}
.content_course_study li:hover cite{display: block;}
.content_course_study li.course_msg_left,.content_course_study li.course_msg_right{position: relative;}
.content_course_study li.course_msg_left cite{right: 0;left: auto;width: auto;}
.content_course_study li.course_msg_right cite{width: auto;}



.course_open{color: #8bc942;}
.course_going{color: #0490d2;}
.course_completed{color: #f58a23;}
.course_lock{color: #858585;}
.content_course_study i.icon-time{color: #8bc942;}
.content_course_study i.icon-play{color: #0490d2;}
.content_course_study i.icon-ok  {color: #f58a23;}
.content_course_study i.icon-lock{color: #858585;}


.content_course_progress{line-height: 20px;padding: 15px 30px;margin-bottom: -20px;}
.content_course_progress span{display: inline-block;padding: 0 5px;}
.content_course_progress i{font-size: 12px;margin-right: 3px;}
.content_course_progress em{color: #333;}



.content_project>p{margin: 20px 10px;padding: 25px;font-size: 14px;line-height: 16px;background: #f1f3f3;}
.content_project dl{margin: 0 10px;}
.content_project dt{margin-top: 10px; font-size: 16px;}
.content_project dt i{margin-right: 5px;}
.content_project dd{font-size: 14px;line-height: 16px;padding: 8px 20px;}


.content_project_relation{position: relative;text-align: center;margin-top: 30px;padding-bottom: 10px;overflow: hidden;}
.content_project_relation h3{position: relative; border-bottom: 1px solid #ec5a2e;padding-bottom: 35px;}
.content_project_relation h3 span{display: inline-block;position: relative; width: 200px;height: 60px;padding-top: 20px;background: #ec5a2e;font-size: 18px;line-height: 20px;color: #fff;font-weight: normal;-moz-border-radius:10px;-webkit-border-radius:10px;border-radius: 10px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_project_relation h3 i{position: absolute;bottom: 20px;left: 50%;margin-left: -5.5px; width: 11px;height: 11px;background: #ec5a2e;-moz-border-radius:5.5px;-webkit-border-radius:5.5px;border-radius: 5.5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_project_relation h3:after{content: '';position: absolute;left: 50%;margin-left: -0.5px;bottom: 0; width: 1px;height: 20px;background: #ec5a2e;}

.content_project_relation cite{float: left;position: relative; width: 33.33334%;padding-top: 40px;}
.content_project_relation cite span{display: block;position: relative;margin: 0 20px 0;background: #42c8e8;padding: 20px;  word-wrap: break-word;font-size: 14px;line-height: 16px;color: #fff; -moz-border-radius:5px;-webkit-border-radius:5px;border-radius: 5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_project_relation cite>i{position: absolute;top: 28px;left: 50%;margin-left: -3.5px; width: 7px;height: 7px;background: #ec5a2e;-moz-border-radius:3.5px;-webkit-border-radius:3.5px;border-radius: 3.5px;behavior:url(/jiaoyu/html/css/pie.htc);}
.content_project_relation cite>i:after{content: '';position: absolute;left: 50%;margin-left: -0.5px;top: -28px; width: 1px;height: 28px;background: #ec5a2e;}
.content_project_relation cite.first:before{content: '';position: absolute;left: 0;top: -2px; width: 50%;height: 3px;background: #fff;}
.content_project_relation cite.last:before{content: '';position: absolute;right: 0;top: -2px; width: 50%;height: 3px;background: #fff;}
.content_project_relation cite em{display: block;margin: 5px 0;  position: relative;padding-top: 50px;}
.content_project_relation cite em:before{content: '';position: absolute;left: 50%;margin-left: -0.5px;top:0; width: 1px;height: 48px;background: #ec5a2e;}
.content_project_relation cite em i{position: absolute;left: 50%;margin-left: -10px;bottom: 0; width: 21px; font-size: 14px;height: 14px;text-align: center;color: #ec5a2e;}



.content_report {overflow: hidden;margin: 20px -10px;}
.content_report li{float: left;width: 50%;font-size: 24px;margin-bottom: 20px; color: #fff;text-align: right;}
.content_report h3{font-size: 40px;margin: 0;line-height: 40px; font-weight: normal;color: #fff;}
.content_report cite{margin: 0 10px;padding: 20px; background: #71d6ee;-moz-border-radius:2px;-webkit-border-radius:2px;border-radius: 2px;}



.content_teamreview{margin: 20px 10px 10px;background: #f1f3f3;padding: 1px 20px 30px;}
.content_teamreview dt{margin-top: 20px; font-size: 16px;margin-bottom: 10px;}
.content_teamreview dt i{margin-right: 5px;}
.content_teamreview dd{margin-left: 15px; font-size: 14px;line-height: 16px;}
.content_teamreview dd:before{content: '';display: inline-block;vertical-align: 3px;margin-right: 5px; width: 4px;height: 4px;background: #585858;-moz-border-radius:2px;-webkit-border-radius:2px;border-radius: 2px;behavior:url(/jiaoyu/html/css/pie.htc);}


.content_faq dl{position: relative;overflow: hidden;}
.content_faq dt{position: relative;margin-top: -1px; font-size: 16px;line-height: 20px;padding: 30px 40px 0;border-top: 1px solid #ddd;}
.content_faq dt:before{content:'Q：';position: absolute;left: 16px;top: 30px;color: #e2291c;}
.content_faq dd{ position: relative;margin-top: 0; font-size: 14px;line-height: 20px; margin-top: 5px;padding: 0px 40px 30px;}
.content_faq dd:before{content:'A：';position: absolute;left: 18px;font-size: 16px; top: 0px;color: #0373a8;}




/* faq dialog */
.dialog_box{display: none; position:fixed;z-index: 999; left:0;top:0;width: 100%;height: 100%;text-align: center;white-space: nowrap; background:rgba(0,0,0,0.5);filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#66000000,endColorstr=#66000000);}
.dialog_box:before{content: '';display: inline-block;width: 1px; height: 100%;vertical-align: middle;}
.dialog{display: inline-block; vertical-align: middle;position: relative;max-width: 600px; background:#efefef;text-align: left;white-space: normal; overflow:hidden;}
.dialog h2{position: relative; height:44px;line-height: 44px;text-indent:1em;font-weight: normal; background: #80888a;color: #fff;}
.dialog h2 i {font-size:16px;position:absolute;right:15px;top:0px;cursor:pointer;z-index:9;color: #fff;}
.dialog>p{font-size: 14px;line-height: 26px;padding: 24px 30px;border-bottom: 2px solid #dcdcdc;}
.dialog label{float: left; min-width: 50%;margin-top: 20px;line-height: 28px;}
.dialog label span{float: left;margin-right: 5px;width: 100px;text-align: right;}
.dialog label span em{vertical-align: middle;margin-right: 3px; color: #e2291c;}
.dialog input[type=text]{width: 150px;height: 18px;line-height: 18px;padding: 3px;border: 1px solid #dddcdd;}
.dialog select{width: 160px;}
.dialog textarea{vertical-align: top;width: 100%;height: 60px;overflow: auto; border: 1px solid #dddcdd;}
.dialog_textarea{position: relative;padding-top: 20px; clear:both;margin-left: 105px;margin-right: 40px;}
.dialog_textarea span{position: absolute; left: -105px;text-align: right;width: 100px; }
.dialog_textarea p{text-align: right;height: 0;}
.dialog_file {display: inline-block;position: relative;overflow: hidden;}
.dialog_file b{display: inline-block; width: 150px;height: 18px;line-height: 18px;padding: 3px;font-weight: normal; background: #fff;border: 1px solid #dddcdd;}
.dialog_file input{position: absolute;right: 0;top: 0;font-size: 200px;outline:none;opacity: 0;filter:alpha(opacity=0);}
.dialog_file i{color: #e2291c;font-size: 16px;margin-left: 10px;}
.dialog_submit {text-align: center;clear: both;padding-top: 30px;}
.dialog_submit button{width: 188px;height: 28px;background: #e2291c;color: #fff;border:none;border-radius: none;}

.dialog .forum_richtext{ padding: 20px;background: #fff;overflow: hidden;margin: 0;}
.dialog .forum_richtext .input_width{position: relative; margin: 0 4px 10px;}
.dialog .forum_richtext .input_width input{width: 100%; margin-left: -4px;padding: 8px 3px;}
.dialog .forum_richtext input[type=submit]{background: #05acfa;}

/*learning forum article*/
.forum {position: relative; border: 1px solid #e9e9e9;padding: 15px;}
.forum_man{position: relative;margin-left: 70px;min-height: 45px;}
.forum_man_pic{position: absolute;left: -55px;top: 0;}
.forum_man_pic img{width: 45px;height: 45px;-moz-border-radius:23px;-webkit-border-radius:23px;border-radius: 23px;behavior:url(/jiaoyu/html/css/pie.htc);}
.forum_man_pic span{display: block;text-align: center;font-size: 12px;}
.forum_man h3{font-size: 16px;font-weight: normal;line-height: 24px;}
.forum_man h6{margin-bottom: 10px; color: #8b8888;font-weight: normal;}
.forum_man h6 i{color: #0caffa;margin-right: 3px;}
.forum_reply{display: block;text-align: right; margin-top: 15px; overflow: hidden;line-height: 28px;}
.forum_reply a{float: left;color: #05acfa;margin-right: 10px;}
.forum_reply a i{color: #6c6d6d;}
.forum_reply button,.forum_reply .btn{width: 103px;height: 28px;line-height: 28px;float: none;margin: 0;padding: 0; border: none; background: #e2291c; color: #fff;text-align: center;}
.forum_tips{position: absolute;right: 15px;top: 5px;color: #e2291c;}
.forum_word{position: relative; margin-left: 45px; margin-top: 20px;background: #f2f7fb;padding: 15px;}
.forum_word:before{content: '';position: absolute;left: 15px;top: -10px; border-style: solid;border-width: 0 10px 10px ;border-color: transparent transparent #f2f7fb;}
.forum_word_time{position: absolute;right: 15px;top: 5px; color: #c6cacd;}
.forum_word_edit{position: absolute;right: 15px;top: 25px;font-size: 14px;color: #e2291c}
.forum_word .forum_reply button,.forum_word .forum_reply .btn{background: #fff;border:1px solid #e0e5e8;color: #585858;}

.forum_fold {margin-left: 45px;margin-top: 8px;}
.forum_fold h3{border-bottom: 1px solid #f2f2f2;text-align: right;font: 14px;height: 16px;line-height: 16px;color: #c1c1c1;font-weight: normal;}
.forum_fold h3 span{display: inline-block;padding:0 20px;cursor: pointer; white-space: nowrap;height: 16px;background: #fff;font-size: 12px; border:1px solid #f2f2f2;border-bottom: none;}
.forum_fold h3 span:after{content: '';display: inline-block;vertical-align: top;margin: 5px; width: 0;height: 0;overflow: hidden;border-style: solid;border-width: 0 5px 5px ;border-color: transparent transparent #c1c1c1;}
.forum_fold ul{line-height: 16px;color: #b4b4b4;white-space: nowrap;}
.forum_fold li{overflow: hidden;padding: 6px 0;}
.forum_fold ul span{display: inline-block;vertical-align: top; white-space: normal;}
.forum_fold ul em{color: #05acfa;}
.forum_fold ul input{width: 100%;line-height: 18px;height: 18px;overflow: auto; border:1px solid #ddd;padding: 3px; color: #999;margin-bottom: 10px;}
.forum_fold ul button{float: right;width: 103px;height: 28px;border: none; text-align: center;background: none; border:1px solid #e0e5e8;color: #05acfa;}

.forum_fold .foat1{ width: 50%;}
.forum_fold .foat2{ width: 30%;text-align: center;}
.forum_fold .foat3{ width: 19%;text-align: right;color: #666;}
.forum_fold .input_width{display: block; position: relative; margin: 0 4px;}
.input_width input{margin-left: -4px;}


.forum_man .forum_word{margin-left: 10px;}
.forum_man .forum_word:before{left: -10px;top: 10px; border-width: 7px 10px 7px 0;border-color: transparent #f2f7fb; }

.forum_richtext{margin: 0 5px;}
.forum_richtext h3{font-size: 16px;font-weight: normal;margin-bottom: 10px;}
.forum_richtext h3 i{color: #0aaefa;margin-right: 3px;}
.forum_richtext textarea {width: 100%;height: 320px;overflow: auto;margin-left: -4px;padding: 4px;}

.forum_richtext input[type=submit]{float: right; width: 103px;height: 28px;line-height: 28px;margin-top: 10px;outline: none; border: none; background: #e2291c; color: #fff;text-align: center;}
.forum_richtext input[type=submit].success{background: #2CAD10;}


.personal_repoert{text-align: left;}
.content .personal_repoert tbody tr:nth-child(even) td,.content .personal_repoert tbody tr:nth-child(even) th {background-color: #fff;}
 

